{
  "name": "discit",
  "version": "1.0.0",
  "description": "A responsive disc golf disc search engine.",
  "author": "Chris Leveille <cdleveille@gmail.com>",
  "license": "MIT",
  "type": "module",
  "module": "src/server/index.ts",
  "scripts": {
    "prepare": "lefthook install",
    "lint": "biome lint .",
    "format": "biome format --write .",
    "check": "biome check .",
    "build": "bun run scripts/build.ts",
    "build:prod": "bun run scripts/build.ts ENV=production && workbox injectManifest workbox-config.json",
    "compile": "bun build --compile --minify --sourcemap --bytecode src/server/index.ts --outfile main",
    "dev": "vite",
    "start": "bun run src/server/index.ts"
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "@clerk/clerk-react": "^5.30.0",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@evilmartians/lefthook": "^1.11.11",
    "@mui/icons-material": "^7.0.2",
    "@mui/material": "^7.0.2",
    "@tanstack/react-query": "^5.74.4",
    "@types/bun": "latest",
    "@types/react": "^19.1.2",
    "@types/react-dom": "^19.1.2",
    "@vitejs/plugin-react": "^4.4.1",
    "bun-copy-plugin": "^0.2.1",
    "discit-types": "^2.1.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-error-boundary": "^5.0.0",
    "react-hot-toast": "^2.5.2",
    "react-infinite-scroll-hook": "^5.0.2",
    "react-router-dom": "^7.5.2",
    "typescript": "^5.8.3",
    "vite": "^6.3.4",
    "vite-tsconfig-paths": "^5.1.4",
    "workbox-cli": "^7.3.0",
    "workbox-routing": "^7.3.0",
    "workbox-strategies": "^7.3.0"
  }
}
